<h1>Listing users</h1>

<a href="http://accounts.spotify.com">Salir de la cuenta</a>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Image</th>
     <!-- <th>Lista De Conciones</th>-->
      <th colspan="5"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class="col-xs-9"><%= user.name %></td>
        <td><%= user.email %></td>
        <td><img src=<%= user.image %> alt="User profile pic" </td>
       
        <td><%= button_to 'Show', user %></td>
        <td><%= button_to 'Edit', edit_user_path(user) %></td>
        <td><%= button_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

